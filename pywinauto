from pywinauto import application
import time

app = application.Application().start('F:\Program Files (x86)\Mobatek\MobaXterm\MobaXterm.exe')
time.sleep(2)

window_title=['MobaXterm']
print(app[window_title].wrapper_object())
print(app[window_title].print_control_identifiers())

